{
    "counter_system": {
        "x": {
            "direction": "bool"
        },
        "y": {
            "count": "int"
        },
        "components": {
            "counter": "counter_model",
            "generator_bool": "direction_generator",
            "generator_int": "increment_generator"
        },
        "eic": [{
            "port_from": "direction",
            "port_to": "direction_in",
            "component_to": "counter_model"
        }],
        "eoc": [{
            "port_from": "count_out",
            "port_to": "count",
            "component_from": "counter_model"
        }],
        "ic": [
            {
            "port_from": "bool_out",
            "port_to": "direction_in",
            "component_to": "counter_model",
            "component_from": "direction_generator"
            },
            {
            "port_from": "int_out",
            "port_to": "increment_in",
            "component_to": "counter_model",
            "component_from": "increment_generator"
            }
        ]
    },
    "include_sets": ["default_sets.json"]
}